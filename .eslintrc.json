{
    "parserOptions": {
        "sourceType": "module"
    },
    "env": {
        "es6": true
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "no-redeclare": ["error"],
        "no-undef": ["error"]
    },
    "globals": {
        "console": "readonly"
    }
}
